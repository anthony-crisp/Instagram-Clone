%h1 Photogram
= link_to "New Post", new_post_path
-@posts.each do |post|
  =image_tag post.image.url(:medium)
  =post.caption
  .posts-wrapper.row
    -@posts.each do |post|
      .post
        .post-head
          .name
            Anthony Crisp
        .image.center-block
          =link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post)
        %p.caption
          =post.caption
